{
"manifest_version": 2,
    "name": "Highlighter",
    "description": "Extension for highlighting words",
    "version": "1.0",
    "permissions": [
        "tabs", "http://*/*", "https://*/*"
    ],
    "browser_action": {
      "default_title": "Enter a search term",
      "default_popup": "popup.html"
  }
}
